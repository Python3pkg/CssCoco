whitespace
{
	require rule newline{2} rule
	require newline eof
    require selector-comma space
	require csv-comma space
	forbid selector whitespace selector
	require selector space "{"
	require declaration newline "}"
	require "{" newline declaration
	require ":" space value
	forbid property whitespace ":"
	forbid space{*} newline
	require declaration (newline or space) declaration
/*
	allow "{" space declaration space "}"
    allow "{" space declaration space declaration space "}"

*/
}