whitespace
{
/*
*/
    require rule newline{2} or newline{3} rule
    require selector space block
    require "{" newline declaration
    require ":" space value

    require declaration newline declaration
	require declaration newline "}"

    forbid space{*} newline{*}
	forbid space{*} eof

    require newline rule newline{2} rule
	require newline eof
}